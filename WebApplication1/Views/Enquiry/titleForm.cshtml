@model FinahProject.Models.EnquiryTitelForm

@using Resources

@{
    ViewBag.Title = "EnquiryForm";
}
@ViewBag.listItems;

@using (Html.BeginForm())
{
    @Html.LabelFor(m => m.titleEnquiry)
    @Html.TextBoxFor(m => m.titleEnquiry)
    @Html.LabelFor(m => m.DescriptionEnquiry)
    
    @Html.TextBoxFor(m => m.DescriptionEnquiry)
    
    @Html.LabelFor(m => m.extraPersonName)
    @Html.TextBoxFor(m => m.extraPersonName)

   
    
    
    
    @Html.ListBoxFor(model => model.extraPerson, Model.ListItems)
    <input type="submit" value="submit" />
}